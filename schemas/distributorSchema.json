{
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "name": "Q & P Electronics",
            "address": "123 Main St., Culver City, MA 12345",
            "phone_number": "(555)555-5555",
            "extenstion": "321",
            "email": "tony@qandp.com",
            "company_contact": "Tony Balogna",
            "website": "qandp.com",
            "logo_url": "http://qandp.com/image.jpg"
        }
    ],
    "required": [
        "name"
    ],
    "properties": {
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Q & P Electronics"
            ]
        },
        "address": {
            "$id": "#/properties/address",
            "type": "string",
            "title": "The address schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "123 Main St., Culver City, MA 12345"
            ]
        },
        "phone_number": {
            "$id": "#/properties/phone_number",
            "type": "string",
            "pattern": "^(?:\\+?(\\d{1,3})[- .]?)?[(]?(\\d{3})(?:(?:\\) )?|[-. )]?)(\\d{3})[-. ]?(\\d{4})$",
            "title": "The phone_number schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "(555)555-5555"
            ]
        },
        "extenstion": {
            "$id": "#/properties/extenstion",
            "type": "string",
            "title": "The extenstion schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "321"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "tony@qandp.com"
            ]
        },
        "company_contact": {
            "$id": "#/properties/company_contact",
            "type": "string",
            "title": "The company_contact schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Tony Balogna"
            ]
        },
        "website": {
            "$id": "#/properties/website",
            "type": "string",
            "title": "The website schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "qandp.com"
            ]
        },
        "logo_url": {
            "$id": "#/properties/logo_url",
            "type": "string",
            "title": "The logo_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "http://qandp.com/image.jpg"
            ]
        },
        "_token": {
            "$id": "#/properties/_token",
            "type": "string",
            "title": "The _token schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "j0f0929joi0w2"
            ]
        }
    },
    "additionalProperties": false
}