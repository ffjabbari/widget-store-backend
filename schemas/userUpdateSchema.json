{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "username": {
      "$id": "/properties/username",
      "type": "string",
      "title": "The Username Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Gilly"]
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "format": "email",
      "minLength": 6,
      "maxLength": 60,
      "title": "The Email Schema ",
      "default": "",
      "examples": ["richardiannucelli@gmail.com"]
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "minLength": 5,
      "maxLength": 55,
      "examples": ["foo123"]
    },
    "first_name": {
      "$id": "/properties/first_name",
      "type": "string",
      "title": "The First_name Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Richard"]
    },
    "last_name": {
      "$id": "/properties/last_name",
      "type": "string",
      "title": "The Last_name Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Iannucelli"]
    },
    "address": {
      "$id": "/properties/address",
      "type": "string",
      "title": "The Street Address Schema ",
      "default": "",
      "minLength": 8,
      "maxLength": 255,
      "examples": ["123 Main St."]
    },
    "apt_number": {
      "$id": "/properties/apt_number",
      "type": "string",
      "title": "The apt_number Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 25,
      "examples": ["Apt. 5E"]
    },
    "state": {
      "$id": "/properties/state",
      "type": "string",
      "title": "The State Schema ",
      "default": "",
      "minLength": 2,
      "maxLength": 30,
      "examples": ["New Hampshire"]
    },
    "postal_code": {
      "$id": "/properties/postal_code",
      "type": "string",
      "title": "The postal_code Schema ",
      "default": "",
      "minLength": 5,
      "maxLength": 10,
      "examples": ["95320-2912"]
    },
    "phone_number": {
      "$id": "#/properties/phone_number",
      "type": "string",
      "pattern": "^(?:\\+?(\\d{1,3})[- .]?)?([(]?(\\d{3})[-. )]?)?(\\d{3})[-. ]?(\\d{4})$",
      "title": "The phone_number schema",
      "description": "User phone number.",
      "default": "",
      "examples": [
          "415-555-5555"
      ]
    },
    "avatar_url": {
      "$id": "/properties/avatar_url",
      "type": "string",
      "title": "The Avatar Schema ",
      "default": "",
      "examples": [
        "https://avatars0.githubusercontent.com/u/13444851?s=460&v=4"
      ]
    },
    "_token": {
      "$id": "/properties/token",
      "type": "string",
      "title": "The Token Schema ",
      "default": "",
      "examples": [
        "foo123"
      ]
    }
  }
}
