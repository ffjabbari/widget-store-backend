{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "_token": ";aljfaljfaj",
            "name": "The Joy of Cooking",
            "description": "Classic cookbook.",
            "image_url": "http://image.com/image.jpg",
            "price": 25.0,
            "quantity": 30,
            "distributor": 1,
            "sku": "S08JF08SJ0-2"
        }
    ],
    "required": [
        "_token",
        "name",
        "description",
        "price"
    ],
    "properties": {
        "_token": {
            "$id": "#/properties/_token",
            "type": "string",
            "title": "The _token schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                ";aljfaljfaj"
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "The Joy of Cooking"
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Classic cookbook."
            ]
        },
        "image_url": {
            "$id": "#/properties/image_url",
            "type": "string",
            "format": "uri",
            "title": "The image_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "http://image.com/image.jpg"
            ]
        },
        "price": {
            "$id": "#/properties/price",
            "type": "number",
            "title": "The price schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0.0,
            "examples": [
                25.0
            ]
        },
        "quantity": {
            "$id": "#/properties/quantity",
            "type": "integer",
            "title": "The quantity schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                30
            ]
        },
        "department": {
            "$id": "#/properties/department",
            "type": "string",
            "title": "The department schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "All Departments"
            ]
        },
        "distributor": {
            "$id": "#/properties/distributor",
            "type": "integer",
            "title": "The distributor schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "sku": {
            "$id": "#/properties/sku",
            "type": "string",
            "title": "The sku schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "S08JF08SJ0-2"
            ]
        },
        "is_active": {
            "$id": "#/properties/is_active",
            "type": "string",
            "title": "The is_active schema",
            "description": "If this is a current searchable product.",
            "default": "",
            "examples": [
                "All Departments"
            ]
        }
    },
    "additionalProperties": false
}